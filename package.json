{
  "name": "@stefanprobst/timer",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": ",/dist/cjs/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/cjs && sh scripts/fix-package-type.sh",
    "prepublishOnly": "npm run build && npm run build:cjs",
    "test": "uvu -r tsm test"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@stefanprobst/commitlint-config": "^1.0.2",
    "@stefanprobst/prettier-config": "^2.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0",
    "tsm": "^2.2.2",
    "typescript": "^4.7.4",
    "uvu": "^0.5.6"
  }
}
